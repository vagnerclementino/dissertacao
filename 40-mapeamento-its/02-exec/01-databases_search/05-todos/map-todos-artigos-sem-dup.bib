@InProceedings{Zimmermann2009,
  Title                    = {Changes and bugs Mining and predicting development activities},
  Author                   = {T. Zimmermann},
  Booktitle                = {Software Maintenance, 2009. ICSM 2009. IEEE International Conference on},
