  Author                   = {R. Ding and Q. Fu and J. G. Lou and Q. Lin and D. Zhang and J. Shen and T. Xie},
  Booktitle                = {Automated Software Engineering (ASE), 2012 Proceedings of the 27th IEEE/ACM International Conference on},
