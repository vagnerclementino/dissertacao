@InProceedings{Azarbakht2014,
  Title                    = {Temporal visualization of collaborative software development in FOSS forks},
  Author                   = {A. Azarbakht},
  Booktitle                = {2014 IEEE Symposium on Visual Languages and Human-Centric Computing (VL/HCC)},
  Year                     = {2014},
